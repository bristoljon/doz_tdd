var meatLoafWouldDo;

meatLoafWouldDo = function(action, reason) {
  var lyric;
  if (action == null) {
    action = "anything";
  }
  if (reason == null) {
    reason = "love";
  }
  if (action !== "that") {
    lyric = "I would do " + action + " for " + reason;
  } else {
    lyric = "But I won't do that";
  }
  return console.log(lyric);
};

meatLoafWouldDo(process.argv[3], process.argv[4]);
